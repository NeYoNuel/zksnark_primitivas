   "source": [
    "def extended_euclid(a, b):\n",
    "    if b == 0:\n",
    "        return a, 1, 0\n",
    "    d1, x1, y1 = extended_euclid(b, a % b)\n",
    "    d = d1\n",
    "    x = y1\n",
    "    y = x1 - (a // b) * y1\n",
    "    return d, x, y\n",
    "\n",
    "\n",
    "print(extended_euclid(973, 301))"
   ]
